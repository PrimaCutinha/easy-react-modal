{
  "name": "easy-react-modal",
  "version": "1.0.43",
  "description": "wrap any react component by this Popup component and turn your component as modal.",
  "main": "./dist/Components/Modal",
  "scripts": {
    "start": "webpack-dev-server --env.local --env.app=./App.js",
    "modal-build:development": "webpack --env.development --env.app=./App.js",
    "modal-build:staging": "webpack --env.staging --env.app=./App.js",
    "modal-build:production": "webpack  --env.production --env.prod --env.app=./App.js",
    "modal-build:publish": "webpack  --env.production --env.prod --env.app=./Components/Modal.js",
    "transpile": "babel ./Components/Modal.js -d dist --copy-files",
    "app-build:dev": "webpack --env.app=app",
    "pretest": "eslint"
  },
  "author": "Kenuete DSouza",
  "keywords": [
    "simple-react-modal",
    "easy-react-modal",
    "React Modal Component",
    "React Modal"
  ],
  "license": "ISC",
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.8.3",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.1.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-transition-group": "^2.4.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^3.12.0"
  }
}
